{
    "homepage": "https://spicecat.github.io/minimal-react-boilerplate/",
    "name": "minimal-react-boilerplate",
    "private": true,
    "version": "0.1.0",
    "devDependencies": {
        "gh-pages": "^5.0.0",
        "react-scripts": "^5.0.1"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-router-dom": "^6.7.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -b gh-pages -d build",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
